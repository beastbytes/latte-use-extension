{
  "name": "beastbytes/latte-use-extension",
  "description": "Latte Use Statement Extension",
  "keywords": [
    "latte",
    "extension"
  ],
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "BSD-3-Clause",
  "require": {
    "php": "^8.1",
    "latte/latte": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "vimeo/psalm": "^5.0",
    "yiisoft/files": "2.0"
  },
  "autoload": {
    "psr-4": {
      "BeastBytes\\Latte\\Extensions\\Use\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BeastBytes\\Latte\\Extensions\\Use\\Tests\\": "tests/"
    }
  }
}
